{
  "name": "turnero-padel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest __tests__/hooks __tests__/lib __tests__/components --verbose",
    "test:integration": "jest __tests__/integration --verbose",
    "test:performance": "jest __tests__/performance --verbose --testTimeout=30000",
    "test:api": "jest __tests__/app/api --verbose",
    "test:services": "jest __tests__/lib/services --verbose",
    "test:utils": "jest __tests__/lib/utils --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:crud": "node __tests__/run-tests.js",
    "test:crud:unit": "jest __tests__/services/crud-service.test.ts --verbose",
    "test:crud:api": "jest __tests__/api/crud.test.ts --verbose",
    "test:crud:integration": "jest __tests__/integration/crud-integration.test.ts --verbose",
    "test:crud:coverage": "jest __tests__ --coverage --coverageReporters=text --coverageReporters=html",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:ui": "playwright test tests/e2e --ui",
    "test:e2e:headed": "playwright test tests/e2e --headed",
    "test:e2e:debug": "playwright test tests/e2e --debug",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:report": "playwright test --reporter=html",
    "test:playwright:headed": "playwright test --headed",
    "test:all": "node scripts/run-all-tests.js",
    "test:all:ci": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:e2e",
    "test:all:crud": "npm run test:crud && npm run test:e2e && npm run test:playwright"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.534.0",
    "next": "^15.5.2",
    "next-auth": "^5.0.0-beta.29",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tanstack/react-query": "^5.85.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.5.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.5",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
